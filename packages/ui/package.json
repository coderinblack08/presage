    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.12",
    "@svgr/webpack": "^5.5.0",
    "autoprefixer": "^10.4.0",
    "file-loader": "^6.2.0",
    "postcss": "^8.3.11",
    "postcss-loader": "4.2.0",
    "tailwindcss": "^2.2.19",
    "tailwindcss-spinner": "^1.2.0"
