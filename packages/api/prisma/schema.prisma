datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id          String   @id @default(uuid()) @db.Uuid
  email       String   @unique
  username    String   @unique
  displayName String
  googleId    String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
